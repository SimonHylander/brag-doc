#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/simonhylander/development/saas/bragdoc/node_modules/.pnpm/@vercel+microfrontends@1.2.4_@vercel+analytics@1.5.0_next@15.5.0_@babel+core@7.28.3_react-dom_6mjhdozho7rohof5cwpjvpvu3a/node_modules/@vercel/microfrontends/cli/node_modules:/Users/simonhylander/development/saas/bragdoc/node_modules/.pnpm/@vercel+microfrontends@1.2.4_@vercel+analytics@1.5.0_next@15.5.0_@babel+core@7.28.3_react-dom_6mjhdozho7rohof5cwpjvpvu3a/node_modules/@vercel/microfrontends/node_modules:/Users/simonhylander/development/saas/bragdoc/node_modules/.pnpm/@vercel+microfrontends@1.2.4_@vercel+analytics@1.5.0_next@15.5.0_@babel+core@7.28.3_react-dom_6mjhdozho7rohof5cwpjvpvu3a/node_modules/@vercel/node_modules:/Users/simonhylander/development/saas/bragdoc/node_modules/.pnpm/@vercel+microfrontends@1.2.4_@vercel+analytics@1.5.0_next@15.5.0_@babel+core@7.28.3_react-dom_6mjhdozho7rohof5cwpjvpvu3a/node_modules:/Users/simonhylander/development/saas/bragdoc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/simonhylander/development/saas/bragdoc/node_modules/.pnpm/@vercel+microfrontends@1.2.4_@vercel+analytics@1.5.0_next@15.5.0_@babel+core@7.28.3_react-dom_6mjhdozho7rohof5cwpjvpvu3a/node_modules/@vercel/microfrontends/cli/node_modules:/Users/simonhylander/development/saas/bragdoc/node_modules/.pnpm/@vercel+microfrontends@1.2.4_@vercel+analytics@1.5.0_next@15.5.0_@babel+core@7.28.3_react-dom_6mjhdozho7rohof5cwpjvpvu3a/node_modules/@vercel/microfrontends/node_modules:/Users/simonhylander/development/saas/bragdoc/node_modules/.pnpm/@vercel+microfrontends@1.2.4_@vercel+analytics@1.5.0_next@15.5.0_@babel+core@7.28.3_react-dom_6mjhdozho7rohof5cwpjvpvu3a/node_modules/@vercel/node_modules:/Users/simonhylander/development/saas/bragdoc/node_modules/.pnpm/@vercel+microfrontends@1.2.4_@vercel+analytics@1.5.0_next@15.5.0_@babel+core@7.28.3_react-dom_6mjhdozho7rohof5cwpjvpvu3a/node_modules:/Users/simonhylander/development/saas/bragdoc/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vercel/microfrontends/cli/index.cjs" "$@"
else
  exec node  "$basedir/../@vercel/microfrontends/cli/index.cjs" "$@"
fi
